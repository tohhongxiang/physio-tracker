{
	"build": {
		"development": {
			"developmentClient": true,
			"distribution": "internal",
			"env": {
				"APP_VARIANT": "development"
			}
		},
		"preview": {
			"android": {
				"buildType": "apk"
			},
			"env": {
				"APP_VARIANT": "debug"
			}
		},
		"production": {
			"env": {
				"APP_VARIANT": "production"
			},
			"autoIncrement": true
		}
	},
	"cli": {
		"version": ">=14.5.0",
		"appVersionSource": "remote"
	}
}
